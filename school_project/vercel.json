{
    "builds":{
        "src":"school_project/wsgi.py",
        "use":"@vercel/python",
        "config":{
            "maxLambdaSize":"15mb",
            "runtime":"python3.11.9"
        }
    },
    "routes":[
        {
            "src":"/(.*)",
            "dest":"school_project/wsgi.py"
        }
    ]
}